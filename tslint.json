{
  "defaultSeverity": "error",
  "extends": ["tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "import-name": [true],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "semicolon": [true, "always", "ignore-bound-class-methods"]
  },
  "rulesDirectory": []
}
